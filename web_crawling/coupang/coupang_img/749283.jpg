    <title>쿠팡!</title>
    <style type="text/css">
        body {background-color:#f0f0f0;font-size:12px;font-family:'돋움';color:#666;}
        #error403{width:720px;margin:130px auto 0;}
        .error-img{background:url("http://img1a.coupangcdn.com/image/www/error/img-error.gif") no-repeat;}
        .error-img.tit{background-position:0 -770px;width:720px;height:143px;margin:0;text-indent:-999em;}
        #contents {width:720px;background-color:#fff;border:1px solid #ccc;margin-bottom:10px;}
        .notice{width:516px;margin:0 auto;background-color:#fff;}
        .call img{vertical-align:middle;margin-right:6px;margin-top:-2px;}
        .btn{width:292px;margin:40px auto 40px;}
        #error403 p{width:360px;margin:33px auto 38px;font:12px/20px '돋움','dotum';color:#555;letter-spacing:-1px;}
